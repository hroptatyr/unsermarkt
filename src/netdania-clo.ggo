args "--unamed-opts --no-handle-error --long-help -a nd_args_info -f nd_parser"
package "netdania"
usage "netdania [OPTION]... SYMBOL..."
description "Subscribe to SYMBOLs on the netdania_fxa data feed."

option "raw" -
	"Display uninterpreted feed"
	optional

section "Network options"
option "beef" -
	"Multicast payload channels, default 7868"
	int optional
